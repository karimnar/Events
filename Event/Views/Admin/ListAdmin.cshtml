@model IEnumerable<Model.Admin>

@{
    Layout = "~/Views/Shared/Admin_Layout.cshtml";}

<h2>List Admin</h2>


<table class="table table-sm">
    <tr>
        <th>
          Name
        </th>
        <th>
           Email
        </th>
      
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nameAdmin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mailAdmin)
        </td>
       
        
        <td>
           @Html.ActionLink("Edit","Edit", new { id=item.idAdmin },new {@class="btn cur-p btn-outline-primary"}) |
            @Html.ActionLink("Delete", "Delete", new { id = item.idAdmin }, new { @class = "btn cur-p btn-outline-danger" })
        </td>
    </tr>
}

</table>
